{
    "name": "jfcherng-roundcube/helper",
    "description": "Some helpers to help develop a Roundcube plugin.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jfcherng\\Roundcube\\Plugin\\Helper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jack Cherng",
            "email": "jfcherng@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix": [
            "php-cs-fixer fix --verbose"
        ]
    }
}
